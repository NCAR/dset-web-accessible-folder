                <gmd:CI_Citation>
                  <gmd:title>
                    <gco:CharacterString>NSIDC Daily Precipitation Totals at Coastal and Island Russian Arctic Stations, 1940-1990</gco:CharacterString>
                  </gmd:title>
                  <gmd:date>
                    <gmd:CI_Date>
                      <gmd:date><gco:Date>2007-01-16</gco:Date></gmd:date>
                      <gmd:dateType>
                        <gmd:CI_DateTypeCode codeList="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#CI_DateTypeCode" codeListValue="publication">publication</gmd:CI_DateTypeCode>
